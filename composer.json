{
    "name": "stev/lista-firme-bundle",
    "description": "Symfony 2 Bundle for Lista Firme Romanian API",
    "type": "symfony-bundle",
    "authors": [{
            "name": "Stefan Matei",
            "email": "stev.matei@gmail.com"
        }],
    "keywords": [
        "lista firme bundle"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": ">=5.3",
        "symfony/framework-bundle": "2.*",
        "symfony/symfony": "2.*",
        "guzzlehttp/guzzle": "6.*",
        "fzaninotto/faker": "1.*",
        "fzaninotto/company-name-generator": "dev-master",
        "symfony/dom-crawler": "3.*",
        "jonnyw/php-phantomjs": "4.*"
    },
    "autoload": {
        "psr-0": {
            "Stev\\ListaFirmeBundle": ""
        }
    },
    "target-dir": "Stev/ListaFirmeBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    },
    "post-install-cmd": [
        "PhantomInstaller\\Installer::installPhantomJS"
    ],
    "post-update-cmd": [
        "PhantomInstaller\\Installer::installPhantomJS"
    ],
    "config": {
        "bin-dir": "bin"
    }
}
