{
    "name": "stev/lista-firme-bundle",
    "description": "Symfony 2 Bundle for Lista Firme Romanian API",
    "type": "symfony-bundle",
    "authors": [{
            "name": "Stefan Matei",
            "email": "stev.matei@gmail.com"
        }],
    "keywords": [
        "lista firme bundle"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "6.* | 7.*",
        "guzzlehttp/guzzle": "7.*",
        "ext-json": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "1.*",
        "fzaninotto/company-name-generator": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Stev\\ListaFirmeBundle": ""
        }
    },
    "target-dir": "Stev/ListaFirmeBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
